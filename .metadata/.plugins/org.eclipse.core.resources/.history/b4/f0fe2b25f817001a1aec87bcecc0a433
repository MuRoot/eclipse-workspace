import java.io.File;

public class DiGui {
	public static void main(String[] args) 
	{
		String path = "D:\\";
		Traverse(path, 0);
	}

	private static void Traverse(String path, int depth) 
	{
		File f = new File(path);
		if(f.exists())
		{
			File[] fils = f.listFiles();
			for(File fv : fils)
			{
				if(fv.isDirectory()) {
					
				}else {
					System.out.println(fv.getName());
				}
			}
		}
	}
}
