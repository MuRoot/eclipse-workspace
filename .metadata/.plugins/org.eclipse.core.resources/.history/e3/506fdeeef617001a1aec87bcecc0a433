import java.io.File;

public class DiGui 
{
	public static void main(String[] args) 
	{
		Traverse("D:\\" , 0);
	}

	private static void Traverse(String path, int depth) 
	{
		File sf = new File(path);
		if(sf.exists())
		{
			File[] files = sf.listFiles();
			for(File fi : files)
			{
				if(fi.isFile()) {
					System.out.println(fi.getName());
				}else {
					Traverse(fi.getAbsolutePath(), (depth+1));
				}
			}
		}
	}
}
