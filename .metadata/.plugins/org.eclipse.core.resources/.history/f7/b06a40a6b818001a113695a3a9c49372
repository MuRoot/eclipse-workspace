public class 第39级台阶 {
	public static int count = 0;

	public static void main(String[] args) 
	{
		new 第39级台阶.dfs(5, 0);
		
		System.out.println("担" + count);
	}

	private void dfs(int n, int step) 
	{
		if (n < 0) {
			return;
		}
		if (n == 0 && step % 2 == 0) {
			count++;
		}
		dfs(n - 1, step + 1);
		dfs(n - 2, step + 1);
	}
}
